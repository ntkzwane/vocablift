            <Font name="Comic Sans MS" size="24.0" />
         </font>
      </TextField>
      <PasswordField fx:id="txt_pwd" layoutX="278.0" layoutY="374.0" prefHeight="64.0" prefWidth="245.0" promptText="Enter password">
