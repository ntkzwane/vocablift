<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" maxHeight="700.0" maxWidth="800.0" minHeight="700.0" minWidth="800.0" prefHeight="700.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vocablift.NumPlaySelController">
   <children>
      <ImageView fitHeight="700.0" fitWidth="800.0" pickOnBounds="true">
         <image>
            <Image url="@green-square-textures-background-3116.jpg" />
         </image>
      </ImageView>
      <Button fx:id="btn_sp" layoutX="84.0" layoutY="58.0" mnemonicParsing="false" onAction="#singlePlayer" prefHeight="158.0" prefWidth="242.0" text="Single Player">
         <font>
            <Font name="Comic Sans MS" size="32.0" />
         </font></Button>
      <Button fx:id="btn_mp" layoutX="470.0" layoutY="58.0" mnemonicParsing="false" onAction="#multiPlayer" prefHeight="158.0" prefWidth="242.0" text="Multi Player">
         <font>
            <Font name="Comic Sans MS" size="32.0" />
         </font></Button>
      <Button fx:id="btn_create" alignment="CENTER" layoutX="65.0" layoutY="275.0" mnemonicParsing="false" onAction="#createGame" prefHeight="188.0" prefWidth="281.0" text="Create&#10;Game">
         <font>
            <Font name="Comic Sans MS" size="49.0" />
         </font>
      </Button>
      <Button fx:id="btn_join" alignment="CENTER" layoutX="451.0" layoutY="275.0" mnemonicParsing="false" onAction="#joinGame" prefHeight="188.0" prefWidth="281.0" text="Join&#10;Game">
         <font>
            <Font name="Comic Sans MS" size="49.0" />
         </font>
      </Button>
      <Button fx:id="btn_confirm" layoutX="222.0" layoutY="314.0" mnemonicParsing="false" onAction="#confirm" prefHeight="73.0" prefWidth="357.0" text="Confirm" textAlignment="CENTER">
         <font>
            <Font name="Comic Sans MS" size="38.0" />
         </font>
      </Button>
      <TextField fx:id="txt_ipfield" alignment="CENTER" editable="false" layoutX="33.0" layoutY="106.0" prefHeight="63.0" prefWidth="734.0">
